
\set QUIET 1

\timing on

\set HISTSIZE 2000
\set HISTCONTROL ignoreboth

\pset pager always
\pset null '[NULL]'

\x auto

\set PROMPT1 '%n:%/@%[%033[1;33;40m%]%M%[%033[0m%]%R%# '
\set PROMPT2 '%n:%/@%[%033[1;33;40m%]%M%[%033[0m%]%R%# '

\setenv LESS -imx4

--\set PROMPT1 '%/@%`hostname -s`%R%# '
--\set PROMPT2 '%/@%`hostname -s`%R%# '

\unset QUIET
